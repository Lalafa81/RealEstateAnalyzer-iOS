// MARK: - Analytics Formulas
"analytics_formula_monthly_income" = "Ежемесячный доход = Годовой доход / Количество месяцев";
"analytics_formula_monthly_expenses" = "Ежемесячные расходы = Годовой расход / Количество месяцев";
"analytics_formula_profit_per_m2" = "Рентабельность м² = (Средний доход - Средний расход) / Площадь";
"analytics_formula_efficiency" = "Коэффициент эффективности = Средний доход / Площадь";
"analytics_formula_roi" = "ROI = ((Средний доход - Средний расход) × 12 / Цена покупки) × 100%";
"analytics_formula_cap_rate" = "Cap Rate = (NOI / Цена покупки) × 100%\n\nNOI = Годовой доход - Годовой расход - Налоги - Страхование";
"analytics_formula_grm" = "GRM = Цена покупки / Годовой доход";
"analytics_formula_payback" = "Окупаемость = Цена покупки / Годовой чистый cash flow\n\nГодовой cash flow = (Средний доход - Средний расход) × 12";
"analytics_formula_irr" = "IRR — ставка дисконтирования, при которой NPV = 0\n\nNPV = -Инвестиция + Σ(Годовой cash flow / (1 + IRR)^год) + Цена продажи / (1 + IRR)^срок_владения";
"analytics_formula_equity_multiple" = "Equity Multiple = (Годовой cash flow × Срок владения + Цена продажи) / Цена покупки";
"analytics_formula_tenant_risk" = "Риск оценивается по максимальному сроку контракта:\n• < 1 года → высокий риск\n• 1-3 года → средний риск\n• > 3 лет → низкий риск";
"analytics_formula_busy_percent" = "Загруженность = (Месяцы с доходом > 0 / Общее количество месяцев) × 100%";
"analytics_formula_volatility" = "Волатильность = Стандартное отклонение доходов / Среднее значение доходов\n\n• < 0.1 → низкая волатильность\n• 0.1-0.5 → умеренная\n• > 0.5 → высокая";
"analytics_formula_concentration" = "Концентрация = (Максимальный доход арендатора / Сумма всех доходов арендаторов) × 100%";
"analytics_formula_break_even" = "Break-Even Occupancy = (Годовой расход / Годовой доход) × 100%";
"analytics_formula_own_years" = "Срок владения = (Текущая дата - Дата покупки) / 365";
"analytics_formula_max_income" = "Максимальный доход = max(доходы по всем месяцам)\n\nОпределяется месяц и год с наибольшим значением дохода.";
"analytics_formula_max_expense" = "Максимальный расход = max(расходы по всем месяцам)\n\nОпределяется месяц и год с наибольшим значением расходов.";
"analytics_formula_income_expense_ratio" = "Доход / Расход = Годовой доход / Годовой расход\n\nЗначение > 1 означает прибыльность, < 1 — убыточность.";

