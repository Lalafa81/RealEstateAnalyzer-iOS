// MARK: - Analytics Formulas
"analytics_formula_monthly_income" = "Monthly Income = Annual Income / Number of Months";
"analytics_formula_monthly_expenses" = "Monthly Expenses = Annual Expenses / Number of Months";
"analytics_formula_profit_per_m2" = "Profit per m² = (Average Income - Average Expense) / Area";
"analytics_formula_efficiency" = "Efficiency Coefficient = Average Income / Area";
"analytics_formula_roi" = "ROI = ((Average Income - Average Expense) × 12 / Purchase Price) × 100%";
"analytics_formula_cap_rate" = "Cap Rate = (NOI / Purchase Price) × 100%\n\nNOI = Annual Income - Annual Expenses - Taxes - Insurance";
"analytics_formula_grm" = "GRM = Purchase Price / Annual Income";
"analytics_formula_payback" = "Payback Period = Purchase Price / Annual Net Cash Flow\n\nAnnual Cash Flow = (Average Income - Average Expense) × 12";
"analytics_formula_irr" = "IRR is the discount rate at which NPV = 0\n\nNPV = -Investment + Σ(Annual Cash Flow / (1 + IRR)^year) + Sale Price / (1 + IRR)^holding_period";
"analytics_formula_equity_multiple" = "Equity Multiple = (Annual Cash Flow × Holding Period + Sale Price) / Purchase Price";
"analytics_formula_tenant_risk" = "Risk is evaluated by maximum contract duration:\n• < 1 year → high risk\n• 1-3 years → medium risk\n• > 3 years → low risk";
"analytics_formula_busy_percent" = "Occupancy = (Months with Income > 0 / Total Number of Months) × 100%";
"analytics_formula_volatility" = "Volatility = Standard Deviation of Income / Average Income\n\n• < 0.1 → low volatility\n• 0.1-0.5 → moderate\n• > 0.5 → high";
"analytics_formula_concentration" = "Concentration = (Maximum Tenant Income / Sum of All Tenant Incomes) × 100%";
"analytics_formula_break_even" = "Break-Even Occupancy = (Annual Expenses / Annual Income) × 100%";
"analytics_formula_own_years" = "Holding Period = (Current Date - Purchase Date) / 365";
"analytics_formula_max_income" = "Maximum Income = max(incomes for all months)\n\nDetermines month and year with highest income value.";
"analytics_formula_max_expense" = "Maximum Expense = max(expenses for all months)\n\nDetermines month and year with highest expense value.";
"analytics_formula_income_expense_ratio" = "Income / Expense = Annual Income / Annual Expenses\n\nValue > 1 means profitability, < 1 means loss.";

